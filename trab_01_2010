14)

#include <stdio.h>
#include <conio.h>
int main( )
{
int b,a,p,A;
printf("Enscreva o valor da base do retangulo:");
scanf("%d",&b);
printf("Enscreva a altura do retangulo:");
scanf("%d",&a);
p=(b+a)*2;
A=b*a;
printf("O perimetro e: %d e A area e: %d",p,A);
getch();
return 0;
}

