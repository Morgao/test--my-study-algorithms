7)

#include <stdio.h>
#include <conio.h>
int main( )
{
int a,b,x,y;
printf("Enscreva um numero Inteiro:");
scanf("%d",&a);
printf("Enscreva outro numero Inteiro:");
scanf("%d",&b);
x=a/b;
y=a%b;
printf(" %d / %d = %d   e   %d %% %d = %d  ", a, b, x, a, b, y);
getch();
return 0;
}

