return 0;
}

18)

#include <stdio.h>
#include <conio.h>
int main( )
{
int r,h,v;
printf("De o raio da basa da lata de oleo:");
scanf("%d",&r);
printf("De a altura da lata de oleo:");
scanf("%d",&h);
v=3.14*(r*r)*h;
printf("O volume da lata de oleo e: %d",v);
getch();
return 0;
}

