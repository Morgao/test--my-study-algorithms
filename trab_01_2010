x=7;
y=8;
z=9;
r=(x+y+z)/3;
printf("A media entre esses numeros e: %.f",r);
getch();
return 0;
}

11)

#include <stdio.h>
#include <conio.h>
int main( )
{
float x,y,z,a,r;
printf("Enscreva um numero real:");
scanf("%f",&x);
printf("Enscreva outro numero real:");
scanf("%f",&y);
printf("Enscreva outro numero real:");
scanf("%f",&z);
printf("Enscreva outro numero real:");
scanf("%f",&a);
r=(x+y+z+a)/4;
printf("A media e: %.1f",r);
getch();
return 0;
}

