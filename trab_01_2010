16)

#include <stdio.h>
#include <conio.h>
int main( )
{
int x,y,r,A;
printf("Primeiro numero da P.A:");
scanf("%d",&x);
printf("Segundo numero da P.A:");
scanf("%d",&y);
r=(y-x);
A=9*r+x;
printf("O decimo termo da serie e: %d",A);
getch();
return 0;
}

