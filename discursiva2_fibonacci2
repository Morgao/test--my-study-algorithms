#include <stdio.h>
#include <stdlib.h>
    void main(){
        int f1 = 0, f2 = 1, fn = 0, n;
        fn = f1 + f2;
        while(fn <= 50000){
            fn++;
           if (fn%3 == 0 &&  fn%7 == 0){
          printf ("%d\n", fn);}
            f1 = f2; f2 = fn; fn = f1 + f2; }
    return 0;}

